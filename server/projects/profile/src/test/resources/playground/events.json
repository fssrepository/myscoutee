[
    {
        "$match": {
            "type": "E",
            "members": {
                "$elemMatch": {
                    "profile.$id": {
                        "$binary": {
                            "base64": "VMzv/kCdSqqaIxrXa9pNLg==",
                            "subType": "04"
                        }
                    },
                    "status": {
                        "$ne": "I"
                    }
                }
            }
        }
    },
    {
        "$match": {
            "$expr": {
                "$and": [
                    {
                        "$or": [
                            {
                                "$gt": [
                                    "$range.start",
                                    {
                                        "$toDate": "2023-11-01T00:00:00Z"
                                    }
                                ]
                            },
                            {
                                "$and": [
                                    {
                                        "$eq": [
                                            "$range.start",
                                            {
                                                "$toDate": "2023-11-01T00:00:00Z"
                                            }
                                        ]
                                    },
                                    {
                                        "$lt": [
                                            "$createdDate",
                                            {
                                                "$toDate": "2023-11-01T00:00:00Z"
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "$in": [
                            "$status",
                            [
                                "A",
                                "P",
                                "C"
                            ]
                        ]
                    }
                ]
            }
        }
    },
    {
        "$sort": {
            "range.start": 1,
            "createdDate": -1
        }
    },
    {
        "$limit": 20
    },
    {
        "$addFields": {
            "groupKey": {
                "$dateToString": {
                    "format": "%Y %U",
                    "date": "$range.start"
                }
            }
        }
    },
    {
        "$addFields": {
            "user": {
                "$first": {
                    "$filter": {
                        "input": "$members",
                        "as": "member",
                        "cond": {
                            "$and": [
                                {
                                    "$eq": [
                                        "$$member.profile.$id",
                                        {
                                            "$binary": {
                                                "base64": "VMzv/kCdSqqaIxrXa9pNLg==",
                                                "subType": "04"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    {
        "$lookup": {
            "from": "likes",
            "let": {
                "p_id": {
                    "$binary": {
                        "base64": "VMzv/kCdSqqaIxrXa9pNLg==",
                        "subType": "04"
                    }
                },
                "p_eventId": "$_id"
            },
            "pipeline": [
                {
                    "$match": {
                        "$expr": {
                            "$and": [
                                {
                                    "$eq": [
                                        "$from.$id",
                                        "$$p_id"
                                    ]
                                },
                                {
                                    "$eq": [
                                        "$ref",
                                        "$$p_eventId"
                                    ]
                                }
                            ]
                        }
                    }
                }
            ],
            "as": "likes"
        }
    },
    {
        "$unwind": {
            "path": "$likes",
            "preserveNullAndEmptyArrays": true
        }
    },
    {
        "$replaceRoot": {
            "newRoot": {
                "$mergeObjects": [
                    {
                        "item": "$$ROOT"
                    },
                    {
                        "groupKey": "$groupKey",
                        "role": {
                            "$cond": [
                                {
                                    "$ifNull": [
                                        "$user",
                                        null
                                    ]
                                },
                                "$user.role",
                                null
                            ]
                        },
                        "rate": {
                            "$cond": [
                                {
                                    "$ifNull": [
                                        "$likes",
                                        null
                                    ]
                                },
                                "$likes.rate",
                                null
                            ]
                        },
                        "sortKey": {
                            "$toLong": "$range.start"
                        },
                        "offset": [
                            "$range.start",
                            "$createdDate"
                        ]
                    }
                ]
            }
        }
    }
]