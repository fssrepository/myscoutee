[
    {
        "$match": {
            "status": "P",
            "type": "g",
            "priority": true
        }
    },
    {
        "$addFields": {
            "currentMembers": {
                "$filter": {
                    "input": "$members",
                    "as": "member",
                    "cond": {
                        "$eq": [
                            "$$member.status",
                            "A"
                        ]
                    }
                }
            },
            "invitedMembers": {
                "$filter": {
                    "input": "$members",
                    "as": "member",
                    "cond": {
                        "$and": [
                            {
                                "$eq": [
                                    "$$member.status",
                                    "I"
                                ]
                            },
                            {
                                "$gte": [
                                    {
                                        "$ifNull": [
                                            "$$member.createdDate",
                                            "ISODate()"
                                        ]
                                    },
                                    {
                                        "$toDate": "?1"
                                    }
                                ]
                            }
                        ]
                    }
                },
                "status": {
                    "$cond": [
                        {
                            "$and": [
                                {
                                    "$lt": [
                                        "$range.start",
                                        {
                                            "$add": [
                                                {
                                                    "$toDate": {
                                                        "$date": "$$NOW"
                                                    }
                                                },
                                                {
                                                    "$multiply": [
                                                        "$gracePeriod",
                                                        60,
                                                        1000
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "$lt": [
                                        {
                                            "$add": [
                                                {
                                                    "$size": "$currentMembers"
                                                },
                                                {
                                                    "$size": "$invitedMembers"
                                                }
                                            ]
                                        },
                                        "$capacity.min"
                                    ]
                                }
                            ]
                        },
                        "T",
                        {
                            "$cond": [
                                {
                                    "$and": [
                                        {
                                            "$lt": [
                                                "$range.start",
                                                {
                                                    "$add": [
                                                        {
                                                            "$toDate": {
                                                                "$date": "$$NOW"
                                                            }
                                                        },
                                                        {
                                                            "$multiply": [
                                                                "$gracePeriod",
                                                                60,
                                                                1000
                                                            ]
                                                        }
                                                    ]
                                                }
                                            ]
                                        },
                                        {
                                            "$gte": [
                                                {
                                                    "$add": [
                                                        {
                                                            "$size": "$currentMembers"
                                                        },
                                                        {
                                                            "$size": "$invitedMembers"
                                                        }
                                                    ]
                                                },
                                                "$capacity.min"
                                            ]
                                        }
                                    ]
                                },
                                "A",
                                "P"
                            ]
                        }
                    ]
                }
            }
        },
        {
            "$lookup": {
                "from": "eventItems",
                "let": {
                    "createdBy": "$createdBy",
                    "currentEventItem": "$$CURRENT",
                    "currentMembers": "$currentMembers"
                },
                "pipeline": [
                    {
                        "$match": {
                            "$expr": {
                                "$and": [
                                    {
                                        "$eq": [
                                            "$$currentEventItem.status",
                                            "P"
                                        ]
                                    },
                                    {
                                        "$eq": [
                                            "$status",
                                            "A"
                                        ]
                                    },
                                    {
                                        "$eq": [
                                            "$type",
                                            "g"
                                        ]
                                    },
                                    {
                                        "$ne": [
                                            "$_id",
                                            "$$currentEventItem._id"
                                        ]
                                    },
                                    {
                                        "$in": [
                                            "$$createdBy",
                                            {
                                                "$map": {
                                                    "input": {
                                                        "$filter": {
                                                            "input": "$members",
                                                            "as": "member",
                                                            "cond": {
                                                                "$in": [
                                                                    "$$member.status",
                                                                    [
                                                                        "A"
                                                                    ]
                                                                ],
                                                            }
                                                        }
                                                    },
                                                    "as": "member",
                                                    "in": "$$member.profile.id"
                                                }
                                            }
                                        ]
                                    },
                                    {
                                        "$or"[
                                            {
                                                "$and": [
                                                    {
                                                        "$eq": [
                                                            "$$currentEventItem.rule.mutual",
                                                            true
                                                        ]
                                                    },
                                                    {
                                                        "$gt": [
                                                            {
                                                                "$size": {
                                                                    "$setIntersection": [
                                                                        "$$currentMembers.profile.id",
                                                                        {
                                                                            "$map": {
                                                                                "input": {
                                                                                    "$filter": {
                                                                                        "input": "$members",
                                                                                        "as": "member",
                                                                                        "cond": {
                                                                                            "$in": [
                                                                                                "$$member.status",
                                                                                                [
                                                                                                    "A"
                                                                                                ]
                                                                                            ],
                                                                                        }
                                                                                    }
                                                                                },
                                                                                "as": "member",
                                                                                "in": "$$member.profile.id"
                                                                            }
                                                                        }
                                                                    ]
                                                                }
                                                            },
                                                            0
                                                        ]
                                                    }
                                                ]
                                            },
                                            {
                                                "$and": [
                                                    {
                                                        "$eq": [
                                                            "$$currentEventItem.rule.mutual",
                                                            false
                                                        ]
                                                    },
                                                    {
                                                        "$in": [
                                                            "$$createdBy",
                                                            {
                                                                "$map": {
                                                                    "input": {
                                                                        "$filter": {
                                                                            "input": "$members",
                                                                            "as": "member",
                                                                            "cond": {
                                                                                "$in": [
                                                                                    "$$member.status",
                                                                                    [
                                                                                        "A"
                                                                                    ]
                                                                                ],
                                                                            }
                                                                        }
                                                                    },
                                                                    "as": "member",
                                                                    "in": "$$member.profile.id"
                                                                }
                                                            }
                                                        ]
                                                    }
                                                ]
                                            }
                                        ]
                                    },
                                    {
                                        "$gte": [
                                            {
                                                "$ifNull": [
                                                    "$range.start",
                                                    "ISODate()"
                                                ]
                                            },
                                            {
                                                "$toDate": "?1"
                                            }
                                        ]
                                    },
                                ]
                            }
                        }
                    },
                    {
                        "$group": {
                            "members": 1
                        }
                    }
                ],
                "as": "candidates"
            }
        }
    ]