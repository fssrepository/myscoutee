[
    {
        "$match": {
            "_id": "?0"
        }
    },
    {
        "$lookup": {
            "from": "events",
            "localField": "items.$id",
            "foreignField": "_id",
            "as": "items"
        }
    },
    {
        "$unwind": "$items"
    },
    {
        "$match": {
            "$expr": {
                "$and": [
                    {
                        "$in": [
                            "$status",
                            [
                                "A",
                                "P"
                            ]
                        ]
                    },
                    {
                        "$or": [
                            {
                                "$gt": [
                                    "$range.start",
                                    {
                                        "$toDate": "?#{#param.offset[0]}"
                                    }
                                ]
                            },
                            {
                                "$and": [
                                    {
                                        "$eq": [
                                            "$range.start",
                                            {
                                                "$toDate": "?#{#param.offset[0]}"
                                            }
                                        ]
                                    },
                                    {
                                        "$gt": [
                                            "$createdDate",
                                            {
                                                "$toDate": "?#{#param.offset[1]}"
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        }
    },
    {
        "$sort": {
            "range.start": 1,
            "createdDate": 1
        }
    },
    {
        "$limit": "?#{#param.limit}"
    },
    {
        "$addFields": {
            "groupKey": {
                "$dateToString": {
                    "format": "?#{#param.groupKey}",
                    "date": "$range.start"
                }
            }
        }
    },
    {
        "$addFields": {
            "user": {
                "$first": {
                    "$filter": {
                        "input": "$members",
                        "as": "member",
                        "cond": {
                            "$and": [
                                {
                                    "$eq": [
                                        "$$member.profile.$id",
                                        "?#{#param.id}"
                                    ]
                                },
                                {
                                    "$eq": [
                                        "$$member.status",
                                        "A"
                                    ]
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    {
        "$replaceRoot": {
            "newRoot": {
                "$mergeObjects": [
                    {
                        "item": "$items"
                    },
                    {
                        "groupKey": "$groupKey",
                        "role": {
                            "$cond": [
                                {
                                    "$ifNull": [
                                        "$user",
                                        null
                                    ]
                                },
                                "$user.role",
                                null
                            ]
                        },
                        "sortKey": {
                            "$toLong": "$createdDate"
                        },
                        "offset": [
                            "$range.start",
                            {
                                "$toDate": "$createdDate"
                            }
                        ]
                    }
                ]
            }
        }
    }
]