[
        {
            "$geoNear": {
                "near": "?1",
                "spherical": true,
                "minDistance": ":#{#offset[0]}",
                "distanceField": "distance",
                "distanceMultiplier": 0.001,
                "query": {
                    "_id": {
                        "$nin": ["?0"]
                    },
                    "gender": "?4",
                    "group": "?5"
                }
            }
        },
        {
            "$lookup": {
                "from": "likes",
                "let": {
                    "p_id": "$_id"
                },
                "pipeline": [
                    {
                        "$match": {
                            "$expr": {
                                "$or": [
                                    {
                                        "$and": [
                                            {
                                                "$eq": ["$from.$id", "$$p_id"]
                                            },
                                            {
                                                "$eq": ["$to.$id", "?0"]
                                            }
                                        ]
                                    },
                                    {
                                        "$and": [
                                            {
                                                "$eq": ["$to.$id", "$$p_id"]
                                            },
                                            {
                                                "$eq": ["$from.$id", "?0"]
                                            }
                                        ]
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "$project": {
                            "distance": {
                                "$avg": "$distance"
                            },
                            "type": {
                                "$avg": {
                                    "$switch": {
                                        "branches": [
                                            {
                                                "case": {
                                                    "$and": [
                                                        {
                                                            "$eq": ["$to.$id", "?0"]
                                                        },
                                                        {
                                                            "$eq": ["$double", false]
                                                        }
                                                    ]
                                                },
                                                "then": 2
                                            },
                                            {
                                                "case": {
                                                    "$and": [
                                                        {
                                                            "$eq": ["$from.$id", "?0"]
                                                        },
                                                        {
                                                            "$eq": ["$double", false]
                                                        }
                                                    ]
                                                },
                                                "then": 1
                                            }
                                        ],
                                        "default": 0
                                    }
                                }
                            },
                            "createdDate": {
                                "$max": "$createdDate"
                            },
                            "data": "$$ROOT"
                        }
                    }
                ],
                "as": "likes"
            }
        },
        {
            "$unwind": {
                "path": "$likes",
                "preserveNullAndEmptyArrays": true
            }
        },
        {
            "$match": {
                "$expr": {
                    "$and": [
                        {
                            "$eq": ["$status", "A"]
                        },
                        {
                            "$eq": [
                                {
                                    "$ifNull": ["$likes.type", 0]
                                },
                                ":#{#type}"
                            ]
                        },
                        {
                            "$gte": [
                                {
                                    "$ifNull": ["$likes.distance", 0]
                                },
                                ":#{#offset[1]}"
                            ]
                        }
                    ]
                }
            }
        },
        {
            "$group": {
                "_id": "$_id",
                "likes": {
                    "$avg": "$likes.distance"
                },
                "type": {
                    "$avg": "$likes.type"
                },
                "rate": {
                    "$avg": {
                        "$switch": {
                            "branches": [
                                {
                                    "case": {
                                        "$eq": ["$likes.type", 1]
                                    },
                                    "then": "$likes.data.rate"
                                }
                            ],
                            "default": 0
                        }
                    }
                },
                "groupKey": {
                    "$first": {
                        "$multiply": [
                            {
                                "$floor": [
                                    {
                                        "$divide": ["$distance", "?3"]
                                    }
                                ]
                            },
                            "?3"
                        ]
                    }
                },
                "distance": {
                    "$first": "$distance"
                }
            }
        },
        {
            "$lookup": {
                "from": "events",
                "let": {
                    "p_id": "$_id"
                },
                "pipeline": [
                    {
                        "$match": {
                            "$expr": {
                                "$and": [
                                    {
                                        "$in": [
                                            "$$p_id",
                                            "$info.members.profile.$id"
                                        ]
                                    },
                                    {
                                        "$not": {
                                            "$in": [
                                                "?0",
                                                "$info.members.profile.$id"
                                            ]
                                        }
                                    },
                                    {
                                        "$gt": [
                                            {
                                                "$ifNull": ["$createdDate", "ISODate()"]
                                            },
                                            {"$toDate": ":#{#offset[4]}"}
                                        ]
                                    }
                                ]
                            }
                        }
                    }
                ],
                "as": "events"
            }
        },
        {
            "$unwind": {
                "path": "$events"
            }
        },
        {
            "$unwind": {
                "path": "$events.info.members"
            }
        },
        {
            "$group": {
                "_id": "$events.info.members.profile.$id",
                "rate": {
                    "$avg": "$events.rate"
                },
                "createdDate": {
                    "$min": "$createdDate"
                },
                "groupKey": {
                    "$min": "$groupKey"
                },
                "likes": {
                    "$avg": "$likes"
                },
                "distance": {
                    "$min": "$distance"
                },
                "data": {
                    "$first": "$events.info.members.profile"
                }
            }
        },
        {
            "$match": {
                "$expr": {
                    "$and": [
                        {
                            "$gte": [
                                {
                                    "$ifNull": ["$rate", 0]
                                },
                                ":#{#offset[2]}"
                            ]
                        },
                        {
                            "$gte": [
                                {
                                    "$ifNull": ["$distance", 0]
                                },
                                ":#{#offset[3]}"
                            ]
                        }
                    ]
                }
            }
        },
        {
            "$sort": {
                "groupKey": 1,
                "likes": -1,
                "rate": -1,
                "distance": 1
            }
        },
        {
            "$limit": "?2"
        },
        {
            "$replaceRoot": {
                "newRoot": {
                    "$mergeObjects": [
                        {
                            "profile": "$data"
                        },
                        {
                            "groupKey": "$groupKey",
                            "offset": [
                                "$groupKey",
                                {
                                    "$ifNull": ["$likes", 0]
                                },
                                "$rate",
                                "$distance",
                                "$createdDate"
                            ]
                        }
                    ]
                }
            }
        }
    ]
    