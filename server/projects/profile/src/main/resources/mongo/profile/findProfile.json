[
    {
        "$geoNear": {
            "near": ":#{#loc}",
            "spherical": true,
            "minDistance": "?#{#param.offset[0]}",
            "distanceField": "distance",
            "query": {
                "_id": {
                    "$nin": [
                        "?#{#param.id}"
                    ]
                },
                "gender": ":#{#gender}",
                "group": ":#{#groupId}",
                "status": "A"
            }
        }
    },
    {
        "$lookup": {
            "from": "likes",
            "let": {
                "p_id": "$_id",
                "p_score": "$score"
            },
            "pipeline": [
                {
                    "$match": {
                        "$expr": {
                            "$or": [
                                {
                                    "$and": [
                                        {
                                            "$eq": [
                                                "$from.$id",
                                                "$$p_id"
                                            ]
                                        },
                                        {
                                            "$eq": [
                                                "$to.$id",
                                                "?#{#param.id}"
                                            ]
                                        },
                                        {
                                            "$eq": [
                                                "$type",
                                                "P"
                                            ]
                                        }
                                    ]
                                },
                                {
                                    "$and": [
                                        {
                                            "$eq": [
                                                "$to.$id",
                                                "$$p_id"
                                            ]
                                        },
                                        {
                                            "$eq": [
                                                "$from.$id",
                                                "?#{#param.id}"
                                            ]
                                        },
                                        {
                                            "$eq": [
                                                "$type",
                                                "P"
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                },
                {
                    "$group": {
                        "_id": null,
                        "distance": {
                            "$avg": "$distance"
                        },
                        "ref": {
                            "$first": {
                                "$ifNull": [
                                    "$ref",
                                    0
                                ]
                            }
                        },
                        "direction": {
                            "$avg": {
                                "$switch": {
                                    "branches": [
                                        {
                                            "case": {
                                                "$and": [
                                                    {
                                                        "$eq": [
                                                            "$to.$id",
                                                            "?#{#param.id}"
                                                        ]
                                                    },
                                                    {
                                                        "$in": [
                                                            "$status",
                                                            [
                                                                "A"
                                                            ]
                                                        ]
                                                    }
                                                ]
                                            },
                                            "then": 2
                                        },
                                        {
                                            "case": {
                                                "$and": [
                                                    {
                                                        "$eq": [
                                                            "$from.$id",
                                                            "?#{#param.id}"
                                                        ]
                                                    },
                                                    {
                                                        "$in": [
                                                            "$status",
                                                            [
                                                                "A"
                                                            ]
                                                        ]
                                                    }
                                                ]
                                            },
                                            "then": 1
                                        }
                                    ],
                                    "default": 0
                                }
                            }
                        },
                        "rateAPG": {
                            "$sum": {
                                "$switch": {
                                    "branches": [
                                        {
                                            "case": {
                                                "$and": [
                                                    {
                                                        "$in": [
                                                            "$status",
                                                            [
                                                                "A"
                                                            ]
                                                        ]
                                                    }
                                                ]
                                            },
                                            "then": {
                                                "$divide": [
                                                    1,
                                                    "$rate"
                                                ]
                                            }
                                        }
                                    ],
                                    "default": 0
                                }
                            }
                        },
                        "avgRateD": {
                            "$avg": {
                                "$switch": {
                                    "branches": [
                                        {
                                            "case": {
                                                "$and": [
                                                    {
                                                        "$eq": [
                                                            "$status",
                                                            "D"
                                                        ]
                                                    }
                                                ]
                                            },
                                            "then": "$rate"
                                        }
                                    ],
                                    "default": 0
                                }
                            }
                        },
                        "numD": {
                            "$sum": {
                                "$cond": [
                                    {
                                        "$in": [
                                            "$status",
                                            [
                                                "D"
                                            ]
                                        ]
                                    },
                                    1,
                                    0
                                ]
                            }
                        },
                        "numAPG": {
                            "$sum": {
                                "$cond": [
                                    {
                                        "$in": [
                                            "$status",
                                            [
                                                "A"
                                            ]
                                        ]
                                    },
                                    1,
                                    0
                                ]
                            }
                        },
                        "createdDate": {
                            "$max": "$createdDate"
                        }
                    }
                },
                {
                    "$set": {
                        "numD": {
                            "$cond": {
                                "if": {
                                    "$lt": [
                                        "$numD",
                                        1
                                    ]
                                },
                                "then": 1,
                                "else": {
                                    "$cond": {
                                        "if": {
                                            "$gt": [
                                                "$numD",
                                                50
                                            ]
                                        },
                                        "then": 50,
                                        "else": "$numD"
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "$set": {
                        "numD": {
                            "$divide": [
                                {
                                    "$ln": {
                                        "$add": [
                                            "$numD",
                                            1
                                        ]
                                    }
                                },
                                {
                                    "$ln": {
                                        "$add": [
                                            50,
                                            1
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                },
                {
                    "$set": {
                        "rate": {
                            "$add": [
                                {
                                    "$divide": [
                                        "$numAPG",
                                        "$rateAPG"
                                    ]
                                },
                                {
                                    "$divide": [
                                        {
                                            "$multiply": [
                                                "$avgRateD",
                                                "$numD"
                                            ]
                                        },
                                        10
                                    ]
                                }
                            ]
                        }
                    }
                }
            ],
            "as": "likes"
        }
    },
    {
        "$unwind": {
            "path": "$likes",
            "preserveNullAndEmptyArrays": true
        }
    },
    {
        "$set": {
            "likes.direction": {
                "$ifNull": [
                    "$likes.direction",
                    0
                ]
            },
            "likes.rate": {
                "$ifNull": [
                    "$likes.rate",
                    0
                ]
            },
            "likes.distance": {
                "$ifNull": [
                    "$likes.distance",
                    {
                        "$abs": {
                            "$subtract": [
                                "$score",
                                "?#{#param.offset[1]}"
                            ]
                        }
                    }
                ]
            },
            "createdDate": {
                "$ifNull": [
                    "$createdDate",
                    "ISODate()"
                ]
            }
        }
    },
    {
        "$addFields": {
            "groupKey": {
                "$multiply": [
                    {
                        "$floor": {
                            "$divide": [
                                "$distance",
                                "?#{#param.step}"
                            ]
                        }
                    },
                    "?#{#param.step}"
                ]
            }
        }
    },
    {
        "$match": {
            "$expr": {
                "$and": [
                    {
                        "$eq": [
                            "$likes.direction",
                            ":#{#direction}"
                        ]
                    },
                    {
                        "$or": [
                            {
                                "$gt": [
                                    "$groupKey",
                                    "?#{#param.offset[0]}"
                                ]
                            },
                            {
                                "$and": [
                                    {
                                        "$eq": [
                                            "$groupKey",
                                            "?#{#param.offset[0]}"
                                        ]
                                    },
                                    {
                                        "$or": [
                                            {
                                                "$lt": [
                                                    "$likes.rate",
                                                    "?#{#param.offset[1]}"
                                                ]
                                            },
                                            {
                                                "$and": [
                                                    {
                                                        "$eq": [
                                                            "$likes.rate",
                                                            "?#{#param.offset[1]}"
                                                        ]
                                                    },
                                                    {
                                                        "$or": [
                                                            {
                                                                "$gt": [
                                                                    "$likes.distance",
                                                                    "?#{#param.offset[2]}"
                                                                ]
                                                            },
                                                            {
                                                                "$and": [
                                                                    {
                                                                        "$eq": [
                                                                            "$likes.distance",
                                                                            "?#{#param.offset[2]}"
                                                                        ]
                                                                    },
                                                                    {
                                                                        "$lt": [
                                                                            "$createdDate",
                                                                            {
                                                                                "$toDate": "?#{#param.offset[3]}"
                                                                            }
                                                                        ]
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                ]
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        }
    },
    {
        "$lookup": {
            "from": "events",
            "let": {
                "p_id": "$_id"
            },
            "pipeline": [
                {
                    "$match": {
                        "status": "A",
                        "$expr": {
                            "$and": [
                                {
                                    "$eq": [
                                        {
                                            "$size": {
                                                "$filter": {
                                                    "input": "$members",
                                                    "cond": {
                                                        "$or": [
                                                            {
                                                                "$and": [
                                                                    {
                                                                        "$eq": [
                                                                            "$$this.status",
                                                                            "A"
                                                                        ]
                                                                    },
                                                                    {
                                                                        "$eq": [
                                                                            "$$this.profile.$id",
                                                                            "$$p_id"
                                                                        ]
                                                                    }
                                                                ]
                                                            },
                                                            {
                                                                "$and": [
                                                                    {
                                                                        "$eq": [
                                                                            "$$this.status",
                                                                            "A"
                                                                        ]
                                                                    },
                                                                    {
                                                                        "$eq": [
                                                                            "$$this.profile.$id",
                                                                            "?#{#param.id}"
                                                                        ]
                                                                    }
                                                                ]
                                                            }
                                                        ]
                                                    }
                                                }
                                            }
                                        },
                                        2
                                    ]
                                }
                            ]
                        }
                    }
                },
                {
                    "$sort": {
                        "range.start": 1
                    }
                },
                {
                    "$limit": 1
                }
            ],
            "as": "events"
        }
    },
    {
        "$unwind": {
            "path": "$events",
            "preserveNullAndEmptyArrays": true
        }
    },
    {
        "$addFields": {
            "met": {
                "$cond": {
                    "if": {
                        "$not": {
                            "$in": [
                                {
                                    "$ifNull": [
                                        "$events",
                                        null
                                    ]
                                },
                                [
                                    null,
                                    []
                                ]
                            ]
                        }
                    },
                    "then": true,
                    "else": false
                }
            }
        }
    },
    {
        "$match": {
            "$expr": {
                "$or": [
                    {
                        "$and": [
                            {
                                "$ne": [
                                    1.5,
                                    ":#{#direction}"
                                ]
                            },
                            {
                                "$eq": [
                                    false,
                                    "$met"
                                ]
                            }
                        ]
                    },
                    {
                        "$eq": [
                            1.5,
                            ":#{#direction}"
                        ]
                    }
                ]
            }
        }
    },
    {
        "$sort": {
            "groupKey": 1,
            "likes.rate": -1,
            "likes.distance": 1,
            "createdDate": -1
        }
    },
    {
        "$limit": "?#{#param.limit}"
    },
    {
        "$addFields": {
            "likes.rate": {
                "$ifNull": [
                    "$likes.rate",
                    0
                ]
            }
        }
    },
    {
        "$replaceRoot": {
            "newRoot": {
                "$mergeObjects": [
                    {
                        "profile": "$$ROOT"
                    },
                    {
                        "rate": {
                            "$cond": {
                                "if": {
                                    "$ne": [
                                        2.0,
                                        ":#{#direction}"
                                    ]
                                },
                                "then": "$likes.rate",
                                "else": 0
                            }
                        },
                        "groupKey": "$groupKey",
                        "met": "$met",
                        "offset": [
                            "$groupKey",
                            "$likes.rate",
                            "$likes.distance",
                            "$createdDate"
                        ]
                    }
                ]
            }
        }
    }
]